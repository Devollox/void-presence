{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"baseUrl": ".",
		"outDir": "dist"
	},
	"include": [
		"src",
		"firebase.ts",
		"vite.*.config.ts",
		"src/void-presence-env.d.ts"
	]
}
