{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"baseUrl": ".",
		"outDir": "dist"
	},
	"include": ["src", "vite.*.config.ts", "src/types/void-presence-env.d.ts"]
}
